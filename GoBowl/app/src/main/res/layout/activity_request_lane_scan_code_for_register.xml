<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:paddingBottom="@dimen/activity_vertical_margin"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
tools:context=".activities.RequestLaneScanCodeForRegisterActivity">

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceLarge"
    android:text="CUSTOMER REQUEST LANE"
    android:id="@+id/textView15"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true" />

<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textAppearance="?android:attr/textAppearanceMedium"
    android:text="SCAN CUSTOMER CARD"
    android:id="@+id/textView16"
    android:layout_marginTop="104dp"
    android:layout_below="@+id/textView15"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="SCAN"
    android:id="@+id/button18"
    android:onClick="Scan_onClick"
    android:layout_alignTop="@+id/textView16"
    android:layout_alignRight="@+id/textView15"
    android:layout_alignEnd="@+id/textView15" />


    <Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="REQUEST LANE"
    android:id="@+id/button19"
    android:onClick="RequestLane_onClick"
    android:layout_alignTop="@+id/button20"
    android:layout_alignLeft="@+id/button18"
    android:layout_alignStart="@+id/button18" />

<Button
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="CANCEL"
    android:id="@+id/button20"
    android:onClick="Cancel_onClick"
    android:layout_alignParentBottom="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true"
    android:layout_marginBottom="158dp" />
</RelativeLayout>
